def calculate_grade(score):
    if score >= 90:
        return 'A'
    elif score >= 80:
        return 'B'
    elif score >= 70:
        return 'C'
    elif score >= 60:
        return 'D'
    else:
        return 'F'

# Test the function
student_score = 85
student_grade = calculate_grade(student_score)
print(f"The student's grade is: {student_grade}")